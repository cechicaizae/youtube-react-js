{
  "name": "youtube-react-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "crypto-js": "^4.1.1",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.0.0-rc.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:elec": "cypress run -b electron",
    "cy:run:ch": "cypress run -b chrome",
    "cy:run:fx": "cypress run -b firefox",
    "delete:reports": "rm cypress/reports/* || true",
    "combine:reports": "mochawesome-merge 'cypress/reports/*.json' > e2e-coverage.json",
    "generate:report": "marge e2e-coverage.json",
    "generate:e2e:cov": "npm run combine:reports && npm run generate:report && npm run delete:reports"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^12.8.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
